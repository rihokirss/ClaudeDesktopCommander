{
  "blockedCommands": [
    "format",
    "mount",
    "umount",
    "mkfs",
    "fdisk",
    "dd",
    "sudo",
    "su",
    "passwd",
    "adduser",
    "useradd",
    "usermod",
    "groupadd"
  ],
  "allowedDirectories": [
    "/tmp",
    "/home/username" 
  ],
  "ssh": {
    "host": "your-server-ip",
    "username": "your-username",
    "privateKeyPath": "~/.ssh/id_rsa",
    "port": 22
  }
}